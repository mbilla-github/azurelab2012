{
	"name": "DFCacheTransExample",
	"properties": {
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "AzureSqlTableDept",
						"type": "DatasetReference"
					},
					"name": "DeptIDMax"
				},
				{
					"dataset": {
						"referenceName": "SrcFileDeptDataSet",
						"type": "DatasetReference"
					},
					"name": "SourceDeptData"
				},
				{
					"dataset": {
						"referenceName": "AzureSqlTableCountry",
						"type": "DatasetReference"
					},
					"name": "CountrySrc"
				}
			],
			"sinks": [
				{
					"name": "CachedDeptID"
				},
				{
					"dataset": {
						"referenceName": "AzureSqlTableDept",
						"type": "DatasetReference"
					},
					"name": "sink1"
				},
				{
					"name": "CacheCountry"
				}
			],
			"transformations": [
				{
					"name": "surrogateKey1"
				},
				{
					"name": "derivedColumn1"
				}
			],
			"scriptLines": [
				"source(output(",
				"          dept_id as integer",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     isolationLevel: 'READ_UNCOMMITTED',",
				"     query: 'select max(dept_id) as dept_id from dept',",
				"     format: 'query') ~> DeptIDMax",
				"source(output(",
				"          DEPARTMENT_ID as integer,",
				"          DEPARTMENT_NAME as string,",
				"          MANAGER_ID as integer,",
				"          LOCATION_ID as integer",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     ignoreNoFilesFound: false) ~> SourceDeptData",
				"source(output(",
				"          country_code as string,",
				"          country_name as string",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     isolationLevel: 'READ_UNCOMMITTED',",
				"     format: 'table') ~> CountrySrc",
				"SourceDeptData keyGenerate(output(dept_id as long),",
				"     startAt: 1L,",
				"     stepValue: 1L) ~> surrogateKey1",
				"surrogateKey1 derive(dept_id = toInteger(dept_id + CachedDeptID#outputs()[1].dept_id)) ~> derivedColumn1",
				"DeptIDMax sink(allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true,",
				"     store: 'cache',",
				"     format: 'inline',",
				"     output: false,",
				"     saveOrder: 1,",
				"     mapColumn(",
				"          dept_id",
				"     )) ~> CachedDeptID",
				"derivedColumn1 sink(allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     input(",
				"          DEPARTMENT_ID as integer,",
				"          DEPARTMENT_NAME as string,",
				"          MANAGER_ID as integer,",
				"          LOCATION_ID as integer,",
				"          dept_id as integer",
				"     ),",
				"     deletable:false,",
				"     insertable:true,",
				"     updateable:false,",
				"     upsertable:false,",
				"     format: 'table',",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true,",
				"     errorHandlingOption: 'stopOnFirstError',",
				"     mapColumn(",
				"          DEPARTMENT_ID,",
				"          DEPARTMENT_NAME = country_name,",
				"          MANAGER_ID,",
				"          LOCATION_ID = country_name,",
				"          dept_id",
				"     )) ~> sink1",
				"CountrySrc sink(allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true,",
				"     keys:['country_code'],",
				"     store: 'cache',",
				"     format: 'inline',",
				"     output: false,",
				"     saveOrder: 1,",
				"     mapColumn(",
				"          country_code,",
				"          country_name",
				"     )) ~> CacheCountry"
			]
		}
	}
}