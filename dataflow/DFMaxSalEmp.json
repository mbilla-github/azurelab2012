{
	"name": "DFMaxSalEmp",
	"properties": {
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "SrcblobEmpDataset",
						"type": "DatasetReference"
					},
					"name": "sourceEmp"
				}
			],
			"sinks": [
				{
					"name": "SinkMaxSal"
				}
			],
			"transformations": [
				{
					"name": "aggregateMaxSal"
				}
			],
			"scriptLines": [
				"source(output(",
				"          EMPLOYEE_ID as integer,",
				"          FIRST_NAME as string,",
				"          LAST_NAME as string,",
				"          EMAIL as string,",
				"          PHONE_NUMBER as string,",
				"          HIRE_DATE as string,",
				"          JOB_ID as string,",
				"          SALARY as integer,",
				"          COMMISSION_PCT as string,",
				"          MANAGER_ID as string,",
				"          DEPARTMENT_ID as string",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     ignoreNoFilesFound: false) ~> sourceEmp",
				"sourceEmp aggregate(MaxSalEmp = max(toInteger(SALARY))) ~> aggregateMaxSal",
				"aggregateMaxSal sink(allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true,",
				"     store: 'cache',",
				"     format: 'inline',",
				"     output: true,",
				"     saveOrder: 1,",
				"     mapColumn(",
				"          MaxSalEmp",
				"     )) ~> SinkMaxSal"
			]
		}
	}
}