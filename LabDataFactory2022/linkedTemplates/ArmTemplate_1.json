					],
					"scriptLines": [
						"source(output(",
						"          EMPLOYEE_ID as string,",
						"          FIRST_NAME as string,",
						"          LAST_NAME as string,",
						"          EMAIL as string,",
						"          PHONE_NUMBER as string,",
						"          HIRE_DATE as string,",
						"          JOB_ID as string,",
						"          SALARY as string,",
						"          COMMISSION_PCT as string,",
						"          MANAGER_ID as string,",
						"          DEPARTMENT_ID as string",
						"     ),",
						"     allowSchemaDrift: true,",
						"     validateSchema: false,",
						"     ignoreNoFilesFound: false) ~> EmpDataSet",
						"source(output(",
						"          DEPARTMENT_ID as string,",
						"          DEPARTMENT_NAME as string,",
						"          MANAGER_ID as string,",
						"          LOCATION_ID as string",
						"     ),",
						"     allowSchemaDrift: true,",
						"     validateSchema: false,",
						"     ignoreNoFilesFound: false) ~> DeptDataSet",
						"EmpDataSet, DeptDataSet join(EmpDataSet@DEPARTMENT_ID == DeptDataSet@DEPARTMENT_ID,",
						"     joinType:'inner',",
						"     partitionBy('hash', 1),",
						"     broadcast: 'auto')~> EmpDeptJoin",
						"EmpDeptJoin sink(allowSchemaDrift: true,",
						"     validateSchema: false,",
						"     input(",
						"          {1} as string,",
						"          {66641e13-d19f-4ce5-aafd-9d5d7befa557} as string,",
						"          {Delete SQL database} as string,",
						"          Succeeded as string,",
						"          Administrative as string,",
						"          Informational as string,",
						"          {6/15/2021 4:44} as string,",
						"          {20c6eec9-2d80-4700-b0f6-4fde579a8783} as string,",
						"          {Microsoft Azure Synapse Resource Provider} as string,",
						"          {Microsoft.Sql/servers/databases} as string,",
						"          {synapseworkspace-managedrg-bd2eb25e-aba7-4f43-a25e-d8757194930d} as string",
						"     ),",
						"     partitionFileNames:['EmpDept'],",
						"     umask: 0022,",
						"     preCommands: [],",
						"     postCommands: [],",
						"     mapColumn(",
						"          {1} = EMPLOYEE_ID,",
						"          {66641e13-d19f-4ce5-aafd-9d5d7befa557} = FIRST_NAME,",
						"          {Delete SQL database} = LAST_NAME,",
						"          Succeeded = PHONE_NUMBER,",
						"          Administrative = SALARY,",
						"          Informational = DEPARTMENT_NAME",
						"     ),",
						"     partitionBy('hash', 1)) ~> OutputEmpDept"
					]
				}
			},
